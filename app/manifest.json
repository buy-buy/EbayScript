{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

   "content_scripts": [
   {
     "matches": ["*://*/*"],
     "js": ["jquwery.js" , "contentScript.js"]
   }
 ],
  "manifest_version": 2
}
